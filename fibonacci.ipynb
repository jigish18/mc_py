    "#Fibonacci sequence upto 'a' iterations\n",
    "a = int(input(\"Please enter a positive number: \"))\n",
    "\n",
    "def fn(n):\n",
    "    x = 0\n",
    "    y = 1\n",
    "\n",
    "    for i in range(n):\n",
    "        z = x + y\n",
    "        x = y\n",
    "        y = z\n",
    "        print(z)\n",
    "\n",
    "fn(a)"
   ]
  }
